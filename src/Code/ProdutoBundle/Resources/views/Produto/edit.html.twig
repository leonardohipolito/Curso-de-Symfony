{% extends '::base.html.twig' %}

{% block body %}
<div class="page-header">
  <h1>Editar Produto<small></small></h1>
</div>

<form class="form" action="{{ path('produto_update',{'id':entity.id}) }}" method="post" {{ form_enctype(form) }}>
    {{ form_errors(form) }}
    {{ form_widget(form) }}
    <input type="submit" value="Enviar" />
</form>
{% endblock %}